{
    "name": "holamundo",
    "version": "1.0.0",
    "description": "Hola Mundo en GraphQL",
    "main": "build/server.js",
    "scripts": {
        "start": "node build/server.js",
        "build": "tsc -p . && ncp src/schema build/schema",
        "start:dev": "npm run build:dev",
        "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/onofrimelisa/hola-mundo-graphql"
    },
    "keywords": [
        "graphql",
        "graphql-curso",
        "hola-mundo"
    ],
    "author": "Melisa Ail√©n Onofri <onofrimelisa@gmail.com> (https://onofrimelisa.github.io/myportfolio/)",
    "license": "ISC",
    "dependencies": {
        "apollo-server-express": "^2.11.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "graphql": "^15.0.0",
        "graphql-import-node": "0.0.4",
        "graphql-playground-middleware-express": "^1.7.12",
        "graphql-tools": "^4.0.7",
        "http": "0.0.1-security",
        "ncp": "^2.0.0",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.4",
        "@types/express-graphql": "^0.9.0",
        "@types/graphql": "^14.5.0",
        "@types/node": "^13.11.0"
    }
}
